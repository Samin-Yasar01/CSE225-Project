import java.util.Scanner;
public class DriverClass
{

	public static void main(String[] args) throws Exception 
	{
		int minutes=0, seconds=0, n=0;
	    
	   
	    for (minutes=0; minutes<1; minutes ++)
	    {
	        for (seconds=0; seconds<60; seconds++)
	        {
	            for (n=0; n<25; n++)
	            {
	                System.out.println ("cls");
	                
	                System.out.print (" \t\t\t\t\t\t\t | %d : %d |\n"+seconds);
	                
	            }}
	     seconds=0;
	    }
		
		
		String a;
		System.out.println("1. Banani");
		Scanner loc1 = new Scanner (System.in);
		a = loc1.nextLine();
	    if (a.equals("Banani"))
		
		{
			Banani obj1 = new Banani();
			obj1.banani();
		}
		else if (a.equals("Dhanmondi"))
		{
			Dhanmondi obj2 = new Dhanmondi();
			obj2.dhanmondi();
		}
		else if (a.equals("Mirpur"))
		{
			Mirpur obj3 = new Mirpur();
			obj3.mirpur();
		}
		else 
		{
			throw new Exception("{-Enther the name according the list.-}");
		}
		}
		
		/*switch(a)
