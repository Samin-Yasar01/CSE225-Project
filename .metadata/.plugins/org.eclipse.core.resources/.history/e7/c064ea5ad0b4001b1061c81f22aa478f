import java.util.Scanner;
public class DriverClass
{

	public static void main(String[] args) 
	{
		String a;
		System.out.println("1. Banani");
		Scanner loc1 = new Scanner (System.in);
		a = loc1.nextLine();
		try
		{
			if (a.equals("Banani"))
		
		{
			Banani obj1 = new Banani();
			obj1.banani();
		}
		else if (a.equals("Dhanmondi"))
		{
			Dhanmondi obj2 = new Dhanmondi();
			obj2.dhanmondi();
		}
		else if (a.equals("Mirpur"))
		{
			Mirpur obj3 = new Mirpur();
			obj3.mirpur();
		}
		}
		catch (Exception e)
		{
			System.out.println(e+"Please Enter the name accurate from the list.");
		}
